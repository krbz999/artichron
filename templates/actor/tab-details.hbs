<div class="{{tabs.details.tabCssClass}}" data-group="primary" data-tab="details">
  <!--<fieldset>
    {{thresholdBar value=document.flags.world.points limit=5 max=20}}
  </fieldset>-->
  <fieldset>
    <legend>Flags</legend>
    <div class="form-group">
      <label>Flag Points 1</label>
      <div class="form-fields">
        {{batteryProgress pointsField1 value=document.flags.world.points max=10 name="flags.world.points" disabled=@root.isPlayMode}}
      </div>
    </div>
    <div class="form-group">
      <label>Flag Points 2</label>
      <div class="form-fields">
        {{batteryProgress pointsField2 value=document.flags.world.points2 max=13 name="flags.world.points2" disabled=@root.isPlayMode rgb="0, 200, 0"}}
      </div>
    </div>
    <p class="hint">These are your flags world points or something.</p>
  </fieldset>
  <fieldset>
    <legend>{{notes.field.label}}</legend>
    {{formInput notes.field value=notes.value enriched=notes.enriched height=300 toggled=true}}
  </fieldset>
</div>
