<form autocomplete="off">

  {{!-- Sheet Header --}}
  <header class="sheet-header">
    <img class="profile-img" src="{{actor.img}}" data-edit="img" data-tooltip="{{actor.name}}" height="100" width="100">
    <div class="header-fields">
      <h1 class="charname">
        <input name="name" type="text" value="{{actor.name}}" placeholder="{{localize 'Name'}}">
      </h1>
    </div>
  </header>

  {{!-- Sheet Tab Navigation --}}
  <nav class="sheet-tabs tabs" data-group="primary">
    <a class="item" data-tab="equipped">{{localize "ARTICHRON.ActorTabEquipped"}}</a>
    <a class="item" data-tab="inventory">{{localize "ARTICHRON.ActorTabInventory"}}</a>
    <a class="item" data-tab="parts">{{localize "ARTICHRON.ActorTabParts"}}</a>
    <a class="item" data-tab="elixirs">{{localize "ARTICHRON.ActorTabElixirs"}}</a>
    <a class="item" data-tab="effects">{{localize "ARTICHRON.ActorTabEffects"}}</a>
  </nav>

  {{!-- Sheet Body --}}
  <section class="sheet-body">

    {{!-- Tabs --}}
    <div class="tab equipped" data-group="primary" data-tab="equipped">
      <section>

      </section>
    </div>

  </section>
</form>
