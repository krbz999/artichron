{{#each item.system.damage as |damage idx|}}
<div class="damage">
  <input type="text" name="system.damage.{{idx}}.value" value="{{damage.value}}">
  <select name="system.damage.{{idx}}.type">
    {{selectOptions @root.config.DAMAGE_TYPES selected=damage.type labelAttr="label" localize=true}}
  </select>
</div>
{{/each}}
