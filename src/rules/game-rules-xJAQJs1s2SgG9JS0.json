    {
      "sort": 0,
      "name": "Hindered",
      "type": "text",
      "_id": "uVv9k9xheOkxzBhO",
      "system": {},
      "title": {
        "show": true,
        "level": 1
      },
      "image": {},
      "text": {
        "format": 1,
        "content": "<p>While hindered, you start with one less action point each turn for each level of this condition. You cannot start your turn with less than 1 action point.</p>"
      },
