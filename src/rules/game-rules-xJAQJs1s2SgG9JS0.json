    {
      "sort": 0,
      "name": "Item Attributes",
      "type": "text",
      "_id": "wbKJ6RNfw1cwIGwq",
      "system": {},
      "title": {
        "show": true,
        "level": 1
      },
      "image": {},
      "text": {
        "format": 1,
        "content": "<p>Items have several attributes, each describing unique properties.</p><ul><li><p>Pristine. This armor grants a higher bonus to defenses or skills.</p></li><li><p>Spellcaster. This path allows for using one of the magical base attacks.</p></li></ul>"
      },
