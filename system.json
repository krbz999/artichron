{
  "id": "artichron",
  "title": "Artichron",
  "description": "Artifice Chronicles System",
  "version": "1.0.0",
  "compatibility": {
    "minimum": 11,
    "verified": 11,
    "maximum": 11
  },
  "authors": [
    {
      "name": "Zhell",
      "url": "https://github.com/krbz999",
      "discord": "zhell9201",
      "ko-fi": "https://ko-fi.com/zhell",
      "patreon": "https://patreon.com/zhell"
    }
  ],
  "esmodules": [
    "module/setup.mjs"
  ],
  "styles": [
    "artichron.css"
  ],
  "packs": [
    {
      "name": "actors",
      "label": "Actors",
      "path": "packs/actors",
      "type": "Actor"
    },
    {
      "name": "items",
      "label": "Items",
      "path": "packs/items",
      "type": "Item"
    },
    {
      "name": "rules",
      "label": "Rules",
      "path": "packs/rules",
      "type": "JournalEntry"
    }
  ],
  "packFolders": [
    {
      "name": "Artichron",
      "sorting": "a",
      "packs": [
        "actors",
        "items",
        "rules"
      ]
    }
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
  ],
  "gridDistance": 1,
  "gridUnits": "m",
  "initiative": "1d20",
  "primaryTokenAttribute": "pools.health",
  "secondaryTokenAttribute": "pools.stamina",
  "socket": true,
  "url": "",
  "manifest": "",
  "download": "",
  "license": "LICENSE.txt",
  "changelog": "CHANGELOG.md",
  "flags": {
    "hotReload": {
      "extensions": [
        "json",
        "css",
        "hbs"
      ],
      "paths": [
        "artichron.css",
        "lang/en.json"
      ]
    }
  }
}
